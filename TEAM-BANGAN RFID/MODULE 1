Sub UpdateTimeOut(UID As String, TimeOutVal As String)
    Dim lastRow As Long
    lastRow = Cells(Rows.Count, 1).End(xlUp).Row

    Dim i As Long
    For i = 2 To lastRow ' Assumes headers in row 1
        If Trim(Cells(i, 5).Value) = UID And Cells(i, 9).Value = "" Then
            Cells(i, 9).Value = TimeOutVal ' Column 9 = "Time Out"
            Exit For
        End If
    Next i
End Sub
